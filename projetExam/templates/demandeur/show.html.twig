{% extends 'base.html.twig' %}

{% block title %}Demandeur{% endblock %}

{% block body %}
    <h1>CV</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ demandeur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ demandeur.Nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ demandeur.Prenom }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ demandeur.Age }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ demandeur.Adresse }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ demandeur.Email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ demandeur.Telephone }}</td>
            </tr>
            <tr>
                <th>Specialite</th>
                <td>{{ demandeur.Specialite }}</td>
            </tr>
            <tr>
                <th>Niveau</th>
                <td>{{ demandeur.Niveau }}</td>
            </tr>
            <tr>
                <th>Experience</th>
                <td>{{ demandeur.Experience }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('demandeur_index') }}">Revenir a la liste</a>

    <a href="{{ path('demandeur_edit', {'id': demandeur.id}) }}">Modifier</a>

    {{ include('demandeur/_delete_form.html.twig') }}
{% endblock %}
